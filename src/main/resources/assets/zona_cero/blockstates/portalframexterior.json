{
  "multipart": [
    {
      "apply": {
        "model": "zona_cero:block/portalframexterior_post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "zona_cero:block/portalframexterior_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "zona_cero:block/portalframexterior_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "zona_cero:block/portalframexterior_side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "zona_cero:block/portalframexterior_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "zona_cero:block/portalframexterior_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "zona_cero:block/portalframexterior_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "zona_cero:block/portalframexterior_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "zona_cero:block/portalframexterior_noside",
        "y": 270
      }
    }
  ]
}